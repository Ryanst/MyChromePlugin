{
  "name":"Chrome滚动条助手",
  "version":"1.0",
  "description":"The chrome plugin from Ryanst",
  "permissions": [
    "contextMenus"
  ],
  "background_page": "background.html",
  "icons": {
    "16": "icon.gif"
  },
  "content_scripts":[
     {
      "matches":["http://*/*"],
      "js":["visit.js","jquery/jquery.js"]
     }
   ],
  "browser_action":{
     "default_icon":"icon.gif",
     "default_title":"first plugin",
     "popup":"popup.html"
  }
}